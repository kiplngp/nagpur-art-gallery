<h1>Listing SubCategory</h1>

    		<table class="admintable">
			  <tr class="admintablehd">
			    <td>Name</td>
			    <td width="25%" style="text-align: center !important;">Action</td>
			  </tr>
			
			<% for sub_category in sub_categories %>
			  <tr>
			    <td><%=h sub_category.title %></td>
			    <td><%= link_to 'Edit', edit_sub_category_path(sub_category) %> | <%= link_to 'Destroy', sub_category, :confirm => 'Are you sure?', :method => :delete %></td>
			  </tr>
			<% end %>
			</table>

   